from pokeapi_wrapper import PokeApiWrapper

# instantiate PokeApiWrapper
p = PokeApiWrapper()

pokemon_info = p.get_pokemon_info('bulbasaur')

print(pokemon_info)

pokemon_info = p.get_pokemon_info('deoxys-attack')

print(pokemon_info)

ability_info = p.get_ability_info('mold-breaker')

print(ability_info)